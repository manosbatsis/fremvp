<resources>

    <style name="CardViewStyle" parent="CardView">
		<item name="cardCornerRadius">8dp</item>
		<item name="cardElevation">3dp</item>
		<item name="cardBackgroundColor">@color/colorWhite</item>
	</style>

	<style name="CardViewStyleBlack" parent="CardView">
		<item name="cardCornerRadius">8dp</item>
		<item name="cardElevation">3dp</item>
		<item name="cardBackgroundColor">@color/black</item>
	</style>

	<style name="CardViewStyle_black_two" parent="CardViewStyle">
		<item name="cardBackgroundColor">@color/color_0d0d0d</item>
	</style>

	<style name="CardViewStyleFab" parent="CardView">
		<item name="cardCornerRadius">25dp</item>
		<item name="cardElevation">3dp</item>
		<item name="cardBackgroundColor">@color/greyish_brown</item>
	</style>

	<style name="FullScreenDialogStyle" parent="android:Theme">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowFullscreen">true</item>
		<item name="android:windowIsFloating">false</item>
	</style>

	<!-- Date Picker -->
	<style name="DatePickerBottomDialog" parent="android:Theme.Light">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:windowIsFloating">true</item>
		<item name="android:backgroundDimEnabled">false</item>
	</style>
	<style name="DatePickerDialogAnim" mce_bogus="1" parent="android:Animation">
		<item name="android:windowEnterAnimation">@anim/datepicker_dialog_in</item>
		<item name="android:windowExitAnimation">@anim/datepicker_dialog_out</item>
	</style>

	<!-- For Tool tip -->
	<style name="ToolTipLayoutDefaultStyle">
		<item name="ttlm_padding_top_bottom">@dimen/ttlm_default_padding_top_bottom</item>
		<item name="ttlm_padding_left_right">@dimen/ttlm_default_padding_left_right</item>
		<item name="ttlm_arrow_padding">@dimen/ttlm_default_arrow_padding</item>
		<item name="ttlm_strokeColor">@android:color/transparent</item>
		<item name="ttlm_backgroundColor">@color/color_1a1a1a</item>
		<item name="ttlm_strokeWeight">@dimen/ttlm_default_stroke_weight</item>
		<item name="ttlm_cornerRadius">@dimen/ttlm_default_corner_radius</item>
		<item name="ttlm_arrowRatio">1.4</item>
		<item name="ttlm_overlayStyle">@style/ToolTipOverlayDefaultStyle</item>
		<item name="ttlm_animationStyle">@style/ToolTipAnimation</item>
		<item name="ttlm_textStyle">@style/ToolTipTextStyle</item>
	</style>
	<!-- For Tool tip -->
	<style name="ToolTipTextStyle" parent="txt_opensans_r_10_verylightpink">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">match_parent</item>
		<item name="android:autoLink">all</item>
		<item name="android:linksClickable">true</item>
		<item name="android:gravity">start|top</item>
		<item name="android:elevation">@dimen/ttlm_default_elevation</item>
		<item name="android:translationZ">@dimen/ttlm_default_elevation</item>
	</style>
	<!-- For Tool tip -->
	<style name="ToolTipOverlayDefaultStyle">
		<item name="android:color">?attr/colorAccent</item>
		<item name="ttlm_repeatCount">99</item>
		<item name="ttlm_duration">600</item>
		<item name="android:alpha">0.5</item>
		<item name="android:layout_margin">8dp</item>
	</style>
	<!-- For Tool tip -->
	<style name="ToolTipAnimation">
		<item name="android:windowEnterAnimation">@anim/ttlm_tooltip_anim_enter</item>
		<item name="android:windowExitAnimation">@anim/ttlm_tooltip_anim_exit</item>
	</style>

